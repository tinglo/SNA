#!/home/katsuya/Desktop/SN_web/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'python-louvain==0.3','console_scripts','community'
__requires__ = 'python-louvain==0.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('python-louvain==0.3', 'console_scripts', 'community')()
    )
